$(".contacts").append("<%= escape_javascript(render(:partial => "contact",
	:locals => { :contact => @contact })) %>")
$("#contact<%= @contact.id %>").hide(0);
$(document).ready(function() {
	$(':input','#contact_field')
	 .not(':button, :submit, :reset, :hidden')
	 .val('')
	$("#contact_field").focus()
});
$("#contact<%= @contact.id %>").slideDown('fast');