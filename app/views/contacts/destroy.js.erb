<% unless @contacts.length < 1 %>
	$("#contact<%= @contact.id %>").slideUp('fast');
	setTimeout(function() {
	  $("#contact<%= @contact.id %>").remove();
	}, 1000);
<% else %>
	$(".contact_list").replaceWith("<%= escape_javascript(render(:partial => "empty")) %>")
	$(".empty").hide().fadeIn('fast');
<% end %>

